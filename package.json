{
  "name": "react-chat",
  "version": "0.0.1",
  "description": "simple chat application using Express.js and React.js",
  "private": true,
  "browserslist": [
    "last 1 version",
    "> 1%",
    "Firefox ESR"
  ],
  "scripts": {
    "start": "webpack-dev-server --port 80 --inline --color --client-log-level warn",
    "server-start": "node ./src/backend/server.js",
    "server-watch": "nodemon --watch src/backend/ src/backend/server.js --verbose",
    "watch": "rimraf dist && webpack --mode development --watch --color",
    "dev": "rimraf dist && webpack --mode development  --color",
    "prod": "rimraf dist && webpack --config webpack.config.prod.js --progress --profile --color",
    "clean": "rimraf dist"
  },
  "author": "HypnoticST8",
  "license": "ISC",
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "express": "^4.17.1",
    "normalize.css": "^8.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "autoprefixer": "^9.6.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.19.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "serve": "^11.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.1.0",
    "typescript": "^3.6.3",
    "url-loader": "^2.0.1",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  }
}
